  "author": "Scio Labs <hello@scio.xyz> (https://scio.xyz)",
  "private": true,
  "version": "0.8.1",
  "scripts": {
    "postinstall": "bash postinstall.sh",
    "dev": "NODE_ENV=development POLKADOTJS_DISABLE_ESM_CJS_WARNING_FLAG=1 next dev",
